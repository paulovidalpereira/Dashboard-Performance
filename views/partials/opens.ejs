    <div class="col-lg-4 col-md-6 col-sm-9 col-xs-12">
        <div class="panel panel-default panel-dashboard" >
            <div class="panel-heading">
                <div class="row" ng-class="{'response-blue':(response.ios.opensTrend=='equal'), 'response-green':(response.ios.opensTrend=='up'), 'response-red':(response.ios.opensTrend=='down')}">
                    <div class="col-xs-2">
                        <i class="fa fa-apple fa-2x" ng-show="response.ios.opensLoading=='success'"></i>
                        <i class="fa fa-spinner fa-spin fa-2x" ng-show="response.ios.opensLoading==undefined||response.ios.opensLoading=='process'"></i>
