                    <div class="col-xs-6 text-right response-main">
                        <span id="opensIos"></span>
                    </div>
                    <div class="col-xs-4 text-left response-compare">
                            <span ng-show="response.ios.opensCompare==undefined">Loading...</span>
                            <i class="fa fa-arrow-up fa-lg" ng-show="response.ios.opensTrend=='up'"></i>
                            <i class="fa fa-arrow-right fa-lg" ng-show="response.ios.opensTrend=='equal'"></i>
                            <i class="fa fa-arrow-down fa-lg" ng-show="response.ios.opensTrend=='down'"></i>
                            <span ng-show="response.ios.opensTrend!='equal'">{{response.ios.opensCompare}}</span>
                    </div>
                </div>
