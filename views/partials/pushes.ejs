<!DOCTYPE html>
<div class="row">
    <div class="col-lg-12">
        <h1><span class="small periodPath"><i class="fa fa-clock-o"></i> Du <span class="darkpath">{{startDate|dateFormat:'YYYY-MM-DD'}}</span> au <span class="darkpath">{{endDate|dateFormat:'YYYY-MM-DD'}}</span> par <span class="darkpath">{{perfSettings|granularityFormat}}</span></span></h1>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <h1 class="page-header page-header-first">Derniers push envoyés<br>
            <% if (session.user['ua_authorization'] == undefined) { %>
            <div class="small loading-error">Le chargement des données Urban Airship a échoué.<br>Connectez-vous à votre compte dans la section configuration.</div>
            <% } %>
        </h1>
    </div>
</div>
<% if (session.user['ua_authorization'] != undefined) { %>
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="panel panel-primary no-border-shadow">
            <div class="panel-heading">
                Derniers push envoyés <i class="fa fa-spinner fa-spin fa-lg" ng-show="response.chart1Loading=='undefined'||response.chart1Loading=='process'"></i>
            </div>
            <div class="panel-body">
                <table class="table table-striped table-bordered table-hover" id="chart1">
                    <thead>
                        <tr>
                            <th style="width: 20%">Date</th>
                            <th style="width: 35%">UUID</th>
                            <th style="width: 15%">Type</th>
                            <th style="width: 15%">Envoyés</th>
                            <th style="width: 15%">Reponses</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" style="margin: -30px 0 30px 0">
        <button type="button" class="btn btn-default" onclick="bkPerf.loadMorePushes('chart1')">
            Charger plus de push <i class="fa fa-spinner fa-spin fa-lg" ng-show="response.chart1NextLoading=='process'"></i>
        </button>
    </div>
</div>
<% } %>